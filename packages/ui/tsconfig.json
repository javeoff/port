{
  "extends": "../../tsconfig.eslint.json",
  "compilerOptions": {
    "baseUrl": "./",
    "lib": ["dom", "dom.iterable", "esnext"],
    "target": "esnext",
    "module": "commonjs",
    "jsx": "react-jsx"
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    ".storybook/**/*.js",
    ".storybook/**/*.jsx",
  ],
  "exclude": [
    "node_modules"
  ]
}
